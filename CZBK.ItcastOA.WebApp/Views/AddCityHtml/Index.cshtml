                            $.messager.alert("提示", "填写信息不可为空!~");
                            return;
                        }

                        $("#addGonggFrom").submit();
                    }
                }, {
                    text: '取消',
                    handler: function () {
                        $('#AddGongg').dialog('close');
                    }
                }]
            });
        }
    </script>

</head>
