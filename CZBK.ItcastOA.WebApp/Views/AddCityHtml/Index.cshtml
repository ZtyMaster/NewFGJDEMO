        function FIfems(value)
        {
            var str="";
            switch (value)
            {
                case 0: str = "登录通知";
                    break;
                case 1: str = "通知公告";
                    break;
                case 2: str = "联系方式";
                        break;
                case 3: str = "关于我们";
                    break;
                case 4: str = "联系QQ";
                    break;
                case 5: str = "Logo";
                    break;
                case 6: str = "公告";
                    break;
                case 8: str = "App滚动消息";
                    break;
                default: str = "未知列";
                    break;
            }
            return str;
        }
